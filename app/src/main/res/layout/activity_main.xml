<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.MainActivity">

    <Button
        android:id="@+id/bt_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="test" />

    <com.chinapex.android.datacollect.testAop.CabbageButton
        android:id="@+id/cb_jar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bt_test"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="我是jar包的button" />

    <Button
        android:id="@+id/bt_2fragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cb_jar"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="跳转到FragmentActivity" />

    <Button
        android:id="@+id/bt_2rv_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bt_2fragment"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="跳转到ListRvActivity" />

    <Button
        android:id="@+id/bt_delay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bt_2rv_list"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="发送延时事件" />

    <Button
        android:id="@+id/bt_instant"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bt_delay"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="发送即时事件" />

    <Button
        android:id="@+id/bt_click_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bt_signOut"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="去测试点击事件" />

    <Button
        android:id="@+id/bt_signIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bt_instant"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="登入" />

    <Button
        android:id="@+id/bt_signOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bt_signIn"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="登出" />


</RelativeLayout>